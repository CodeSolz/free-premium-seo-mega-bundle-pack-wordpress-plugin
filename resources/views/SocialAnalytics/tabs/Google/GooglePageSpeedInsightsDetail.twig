{#page start#}
<div class="all-in-one-seo-content">
    <div class="fixed-left">
        {#wrapper start#}
        <div id="wrapper">
            {#include page header#}
            {% include '@CsSeoMegaBundlePack/Global/includes/header.twig' %} 
            {#include page sidebar#}
            {% include '@CsSeoMegaBundlePack/Global/includes/sidebar.twig' %} 
            {#content page start#}
            <div class="content-page">
                {#content div start#}
                <div class="content">
                    {#container div start#}
                    <div class="container">
                        {#page title start#}
                        {% include '@CsSeoMegaBundlePack/Global/includes/title.twig' %} 
                        {#page title end#}
                        {#main content start#}
                        <div class="row">
                            <div id="pagepseed" class="col-lg-12">
                                <div class="panel panel-default">
                                    <div class="panel-heading">
                                        <h3 class="panel-title font-13">
                                            {{ps_panel_title}}
                                            <span class="pull-right">{{last_checked}} {{detail_data.checked_on|date("M d, Y h:i:s A")}}</span>
                                        </h3>
                                    </div>
                                    <div class="panel-body">
                                        <div class="row">
                                            <div class="col-lg-6">
                                                <div class="card-box">
                                                    <div id="graph_desktop_score" class="graph-speed"></div>
                                                </div>
                                            </div>
                                            <div class="col-lg-6">
                                                <div class="card-box">
                                                    <div id="graph_mobile_score" class="graph-speed"></div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div id="pagepseed" class="col-lg-12">
                                <div class="panel panel-warning ">
                                    <div class="panel-heading">
                                        <h3 class="panel-title font-13">
                                            {{panel_po}}
                                            <span class="pull-right">{{last_checked}} {{detail_data.checked_on|date("M d, Y h:i:s A")}}</span>
                                        </h3>
                                    </div>
                                    <div class="panel-body">
                                        <div class="row">
                                            <div class="col-lg-6">
                                                <div class="card-box">
                                                    <h5 class="text-center m-b-20"><b>{{desktop}}</b></h5>
                                                    {% if detail_data.desktop_need_optimization is not empty %}
                                                        {% for key, item in detail_data.desktop_need_optimization %}
                                                            {% if item.header is not empty %}
                                                            <div class="panel-group" id="accordion-desktop-{{key}}">
                                                                <div class="panel panel-warning ">
                                                                    <div class="panel-heading">
                                                                        <h4 class="panel-title">
                                                                            <a data-toggle="collapse" data-parent="#accordion-desktop-{{key}}" href="#collapse-desktop-{{key}}" aria-expanded="false" class="collapsed">
                                                                                <i class="fa fa-warning"></i>{{item.header|raw}}
                                                                            </a>
                                                                        </h4>
                                                                    </div>
                                                                    <div id="collapse-desktop-{{key}}" class="panel-collapse collapse">
                                                                        <div class="panel-body">
                                                                            <p class="">{{item.summary|raw}}</p>
                                                                            <p class="description m-b-15">{{item.summary2|raw}}</p>
                                                                            {{item.content|raw}}
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                                </div>
                                                            {% endif %}
                                                        {% endfor %}
                                                    {% endif %}
                                                </div>
                                            </div>
                                            <div class="col-lg-6">
                                                <div class="card-box">
                                                    <h5 class="text-center m-b-20"><b>{{mobile}}</b></h5>
                                                    {% if detail_data.mobile_need_optimization is not empty %}
                                                        {% for key, item in detail_data.mobile_need_optimization %}
                                                            {% if item.header is not empty %}
                                                            <div class="panel-group" id="accordion-mobile-{{key}}">
                                                                <div class="panel panel-warning ">
                                                                    <div class="panel-heading">
                                                                        <h4 class="panel-title">
                                                                            <a data-toggle="collapse" data-parent="#accordion-mobile-{{key}}" href="#collapse-mobile-{{key}}" aria-expanded="false" class="collapsed">
                                                                                <i class="fa fa-warning"></i> {{item.header|raw}}
                                                                            </a>
                                                                        </h4>
                                                                    </div>
                                                                    <div id="collapse-mobile-{{key}}" class="panel-collapse collapse">
                                                                        <div class="panel-body">
                                                                            <p class="">{{item.summary|raw}}</p>
                                                                            <p class="description m-b-15">{{item.summary2|raw}}</p>
                                                                            {{item.content|raw}}
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                                </div>
                                                            {% endif %}
                                                        {% endfor %}
                                                    {% endif %}
                                                    
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div id="pagepseed" class="col-lg-12">
                                <div class="panel panel-success ">
                                    <div class="panel-heading">
                                        <h3 class="panel-title font-13">
                                            {{panel_of}}
                                            <span class="pull-right">{{last_checked}} {{detail_data.checked_on|date("M d, Y h:i:s A")}}</span>
                                        </h3>
                                    </div>
                                    <div class="panel-body">
                                        <div class="row">
                                            <div class="col-lg-6">
                                                <div class="card-box">
                                                    <h5 class="text-center m-b-20"><b>{{desktop}}</b></h5>
                                                    {% if detail_data.desktop_optimized is not empty %}
                                                        {% for key, item in detail_data.desktop_optimized %}
                                                            {% if item.header is not empty %}
                                                            <div class="panel-group" id="accordion-desktop-{{key}}">
                                                                <div class="panel panel-success ">
                                                                    <div class="panel-heading">
                                                                        <h4 class="panel-title">
                                                                            <a data-toggle="collapse" data-parent="#accordion-desktop-{{key}}" href="#collapse-desktop-{{key}}" aria-expanded="false" class="collapsed">
                                                                                <i class="fa fa-check"></i> {{item.header|raw}}
                                                                            </a>
                                                                        </h4>
                                                                    </div>
                                                                    <div id="collapse-desktop-{{key}}" class="panel-collapse collapse">
                                                                        <div class="panel-body">
                                                                            <p class="description">{{item.summary|raw}}</p>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                                </div>
                                                            {% endif %}
                                                        {% endfor %}
                                                    {% endif %}
                                                </div>
                                            </div>
                                            <div class="col-lg-6">
                                                <div class="card-box">
                                                    <h5 class="text-center m-b-20"><b>{{mobile}}</b></h5>
                                                    {% if detail_data.mobile_optimized is not empty %}
                                                        {% for key, item in detail_data.mobile_optimized %}
                                                            {% if item.header is not empty %}
                                                            <div class="panel-group" id="accordion-mobile-{{key}}">
                                                                <div class="panel panel-success ">
                                                                    <div class="panel-heading">
                                                                        <h4 class="panel-title">
                                                                            <a data-toggle="collapse" data-parent="#accordion-mobile-{{key}}" href="#collapse-mobile-{{key}}" aria-expanded="false" class="collapsed">
                                                                                <i class="fa fa-check"></i> {{item.header|raw}}
                                                                            </a>
                                                                        </h4>
                                                                    </div>
                                                                    <div id="collapse-mobile-{{key}}" class="panel-collapse collapse">
                                                                        <div class="panel-body">
                                                                            <p class="description">{{item.summary|raw}}</p>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                                </div>
                                                            {% endif %}
                                                        {% endfor %}
                                                    {% endif %}
                                                    
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                                    
                        <div class="row">
                            <div id="form_pagepseedtest" class="col-lg-12">
                                <form id="InternalAddNewRule" action="{{form_action}}" role='form' method='post' class="form-horizontal">
                                <div class="panel panel-default">
                                    <div class="panel-heading">
                                        <h3 class="panel-title font-13">
                                            {{panel_title}}
                                            
                                        </h3>
                                    </div>
                                    <div class="panel-body">
                                        {% if is_app_has_set is empty %}
                                            <div class="alert alert-dismissable">
                                                <i class="fa fa-info"></i> {{setup_msg|raw}}
                                            </div>
                                        {% endif %}
                                        <div class="form-group">
                                            <label class="col-sm-3 control-label">{{label_re_url}}</label>
                                            <div class="col-sm-9">
                                                <input class="form-control" type="text" name="test_url" value="{{detail_data.url}}" readonly id="test_url" placeholder ="{{placeholder_test_url}}" required=""/>
                                                <p class="description">{{des_test_url}}</p>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="panel-footer">
                                        <input type="hidden" name="_wpnonce" id="_wpnonce" value="{{nonce_field}}"/>
                                        <button class="btn btn-purple waves-effect waves-light btn-submit" type="submit">{{label_re_submit_btn}}</button>
                                    </div>
                                </div>
                                    </form>
                                </div>
                            </div>
                        
                    </div> {#container end#}
                </div> {#content end#}
                {#include footer#}
                {% include '@CsSeoMegaBundlePack/Global/includes/footer.twig' %} 
            </div> {#page content end#}
        </div> {#wrapper end#}
    </div>
</div> {#page end#}