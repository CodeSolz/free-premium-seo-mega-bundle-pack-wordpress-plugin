{#page start#}
<div class="all-in-one-seo-content">
    <div class="fixed-left">
        {#wrapper start#}
        <div id="wrapper">
            {#include page header#}
            {% include '@CsSeoMegaBundlePack/Global/includes/header.twig' %} 
            {#include page sidebar#}
            {% include '@CsSeoMegaBundlePack/Global/includes/sidebar.twig' %} 
            {#content page start#}
            <div class="content-page">
                {#content div start#}
                <div class="content">
                    {#container div start#}
                    <div class="container">
                        {#page title start#}
                        {% include '@CsSeoMegaBundlePack/Global/includes/title.twig' %} 
                        {#page title end#}
                        {#main content start#}
                        <div class="row">
                            <div class="col-lg-12">
                                <div class="card-box">
                                {% if tbl_rows.error is defined %}
                                    <div class="alert alert-warning">
                                        <i class="fa fa-warning"></i> {{tbl_rows.error|raw}}
                                    </div>
                                {% else %}
                                    
                                {% if no_internet is not empty %}
                                    <div class="card-box">
                                      <div class="alert alert-warning"><i class="fa fa-warning"></i> {{no_internet}}</div>
                                    </div>
                                {% else %}
                                
                                <div class="btn-group" id="demo-delete-row">
                                    <button id="btn_delete" data-action="sitemaps_manage" class="btn btn-danger" disabled="disabled"><i class="fa fa-times m-r-5"></i>{{actn_delete_btn}}</button>
                                    <input type="hidden" name="_wpnonce" id="_wpnonce" value="{{nonce_field}}"/>
                                </div>
                                   
                                <table id="demo-custom-toolbar" data-toggle="table" data-toolbar="#demo-delete-row" data-search="true" 
                                        data-show-columns="true" data-sort-name="id"
                                       data-page-list="[10, 20, 30, 50]" data-page-size="10" data-pagination="true" data-show-pagination-switch="true" class="table-bordered">
                                    <thead>
                                        <tr >
                                            <th data-field="state" data-checkbox="true"></th>
                                            {% for col_name in tbl_headers %}
                                                <th data-field="{{ col_name }}" data-sortable="true">{{ col_name }}</th>
                                            {% endfor %}
                                        </tr>
                                    </thead>
                                    <tbody>
                                        {% for row in tbl_rows %}
                                            <tr id="item_id_{{row.id}}" >
                                                <td><input  type="checkbox" value="{{row.path}}"/></td>
                                                <td>
                                                    <input type="hidden" value="{{row.path}}" class="item_id"/>
                                                    <a href="{{row.path}}">{{row.path}}</a>
                                                </td>
                                                <td>
                                                    {{type}}
                                                </td>
                                                <td>
                                                    {{row.lastSubmitted|date("M d, Y")}}
                                                </td>
                                                <td>
                                                    {{row.errors}}
                                                </td>
                                                <td>
                                                    {{row.warnings}}
                                                </td>
                                                <td class="no-padding-left-right">
                                                    <div class="border-bottom">{{web}}</div>
                                                    <div >{{images}}</div>
                                                </td>
                                                <td class="no-padding-left-right">
                                                    <div class="border-bottom">{% if row.contents.0.submitted is not empty %} {{row.contents.0.submitted}} {% else %} 0 {% endif %}</div>
                                                    <div >{% if row.contents.1.submitted is not empty %} {{row.contents.1.submitted}} {% else %} 0 {% endif %}</div>
                                                </td>
                                                <td class="no-padding-left-right">
                                                    <div class="border-bottom">{% if row.contents.0.indexed is not empty %} {{row.contents.0.indexed}} {% else %} 0 {% endif %}</div>
                                                    <div >{% if row.contents.1.indexed is not empty %} {{row.contents.1.indexed}} {% else %} 0 {% endif %}</div>
                                                </td>
                                                
                                            </tr>
                                        {% endfor %}
                                        </tbody>
                                    </table>
                                    {% endif %}
                                {% endif %}
                            </div>
                            </div>
                        </div>
                        
                        {#main content end#}
                    </div> {#container end#}
                </div> {#content end#}
                {#include footer#}
                {% include '@CsSeoMegaBundlePack/Global/includes/footer.twig' %} 
            </div> {#page content end#}
        </div> {#wrapper end#}
    </div>
</div> {#page end#}