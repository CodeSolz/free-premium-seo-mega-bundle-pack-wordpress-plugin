{#page start#}
<div class="all-in-one-seo-content">
    <div class="fixed-left">
        {#wrapper start#}
        <div id="wrapper">
            {#include page header#}
            {% include '@CsSeoMegaBundlePack/Global/includes/header.twig' %} 
            {#include page sidebar#}
            {% include '@CsSeoMegaBundlePack/Global/includes/sidebar.twig' %} 
            {#content page start#}
            <div class="content-page">
                {#content div start#}
                <div class="content">
                    {#container div start#}
                    <div class="container">
                        {#page title start#}
                        {% include '@CsSeoMegaBundlePack/Global/includes/title.twig' %} 
                        {#page title end#}
                        <div class="row">
                            <div id="google_settings" class="col-lg-12">
                                {% if aios_goo_app_token.auth_status is empty %}
                                    <div class="card-box">
                                        <div class="alert alert-warning">
                                            <i class="fa fa-warning"></i> {{aios_goo_app_token.auth_error_msg|raw}}
                                        </div>
                                    </div>
                                {% else %}
                                    
                                    {% if no_internet is not empty %}
                                        <div class="card-box">
                                          <div class="alert alert-warning"><i class="fa fa-warning"></i> {{no_internet}}</div>
                                        </div>
                                    {% else %}
                                
                                    <div class="panel panel-default">
                                        <div class="panel-heading">
                                            <h3 class="panel-title font-13">
                                                {{panel_title}} - {{result.profileInfo.profileName}} ({{result.profileInfo.webPropertyId}})
                                            </h3>
                                        </div>
                                        <div class="panel-body">
                                            <div class="row m-b-30">
                                                <div class="col-sm-4 p-t-12"><b>{{data_showing}}</b></div>
                                                <div class="col-sm-4 col-sm-offset-4">
                                                    <div id="reportrange" class="form-control">
                                                        <i class="glyphicon glyphicon-calendar fa fa-calendar"></i>
                                                        <span></span>
                                                    </div>
                                                </div>
                                            </div>

                                        </div>
                                    </div>
                                      
                                                
                                    <div class="row">
                                        {% if totalResults is not empty %}
                                            {% for label, count in totalResults %}
                                            <div class="col-lg-3">
                                                <div class="card-box">
                                                    <div class="bar-widget">
                                                        <div class="table-box">
                                                            <div class="table-detail"><div class="iconbox bg-info"><i class="fa fa-users"></i></div></div>
                                                            <div class="table-detail text-right"><h4 class="m-t-0 m-b-5"><b class="counter">{{count}}</b></h4><p class="text-muted m-b-0 m-t-0">{{label}}</p></div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                            {% endfor %}
                                        {% endif %}
                                    </div>            
                                    {#country graph#}
                                    <div class="row">
                                        <div class="col-lg-12">
                                            <div class="card-box">
                                                <div id="graph_audience_overview" class="visits-by-country"></div>
                                            </div>
                                        </div>
                                    </div>
                                    {#country graph#}
                                    
                                    {% if pie_chart_status == 'true' %}
                                    
                                    {#country graph#}
                                    <div class="row">
                                        <div class="col-lg-6">
                                            <div class="card-box">
                                                <div id="graph_audience_country" class="visits-by-country"></div>
                                            </div>
                                        </div>
                                        <div class="col-lg-6">
                                            <div class="card-box">
                                                <div id="graph_audience_source" class="visits-by-country"></div>
                                            </div>
                                        </div>
                                    </div>
                                    {#country graph#}
                                    {#visitor source / device type graph#}
                                    <div class="row">
                                        <div class="col-lg-6">
                                            <div class="card-box">
                                                <div id="graph_audience_device" class="visits-by-country"></div>
                                            </div>
                                        </div>
                                        <div class="col-lg-6">
                                            <div class="card-box">
                                                <div id="graph_audience_browser" class="visits-by-country"></div>
                                            </div>
                                        </div>
                                    </div>
                                    {#visitor source / device type graph#}
                                    {#visitor lang / type graph#}
                                    <div class="row">
                                        <div class="col-lg-6">
                                            <div class="card-box">
                                                <div id="graph_audience_type" class="visits-by-country"></div>
                                            </div>
                                        </div>
                                        <div class="col-lg-6">
                                            <div class="card-box">
                                                <div id="graph_audience_lang" class="visits-by-country"></div>
                                            </div>
                                        </div>
                                    </div>
                                    {#visitor lang / type graph#}
                                    {% endif %}
                                    
                                {% endif %}
                                {% endif %}
                            </div>
                        </div>


                        {#main content start#}

                        {#main content end#}
                    </div> {#container end#}
                </div> {#content end#}
                {#include footer#}
                {% include '@CsSeoMegaBundlePack/Global/includes/footer.twig' %} 
            </div> {#page content end#}
        </div> {#wrapper end#}
    </div>
</div> {#page end#}