{#page start#}
<div class="all-in-one-seo-content">
    <div class="fixed-left">
        {#wrapper start#}
        <div id="wrapper">
            {#include page header#}
            {% include '@CsSeoMegaBundlePack/Global/includes/header.twig' %} 
            {#include page sidebar#}
            {% include '@CsSeoMegaBundlePack/Global/includes/sidebar.twig' %} 
            {#content page start#}
            <div class="content-page">
                {#content div start#}
                <div class="content">
                    {#container div start#}
                    <div class="container">
                        {#page title start#}
                        {% include '@CsSeoMegaBundlePack/Global/includes/title.twig' %} 
                        {#page title end#}
                        {#main content start#}
                        <div class="row">
                            <div class="col-lg-4">
                                <div class="card-box">
                                    <div class="bar-widget">
                                        <div class="table-box">
                                            <div class="table-detail"><div class="iconbox bg-danger"><i class="fa fa-times"></i></div></div>
                                            <div class="table-detail text-right"><h4 class="m-t-0 m-b-5"><b class="counter">{{count_404}}</b></h4><p class="text-muted m-b-0 m-t-0">{{label_404_count}}</p></div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-lg-4">
                                <div class="card-box">
                                    <div class="bar-widget">
                                        <div class="table-box">
                                            <div class="table-detail"><div class="iconbox bg-success"><i class="fa fa-mail-forward"></i></div></div>
                                            <div class="table-detail text-right"><h4 class="m-t-0 m-b-5"><b class="counter">{{count_redirection}}</b></h4><p class="text-muted m-b-0 m-t-0">{{label_redirection_count}}</p></div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-lg-4">
                                <div class="card-box">
                                    <div class="bar-widget">
                                        <div class="table-box">
                                            <div class="table-detail"><div class="iconbox bg-info"><i class="fa fa-link"></i></div></div>
                                            <div class="table-detail text-right"><h4 class="m-t-0 m-b-5"><b class="counter">{{internal_link_rule_count}}</b></h4><p class="text-muted m-b-0 m-t-0">{{label_internal_link}}</p></div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                    </div>
                        <div class="row">
                            <div class="col-lg-4">
                                <div class="card-box">
                                    <div id="donut-chart">
                                        <div id="morris-donut-chart" style="height: 150px;"></div>
                                    </div>
                                    <div class="text-center">
                                        <p style="margin: -5px;"><i class="fa fa-circle m-r-5" style="color: #006400;"></i>{{label_blc_price}} {{total_blc_price}}</p>
                                    </div>
                                </div>
                            </div>   
                            <div class="col-lg-4">
                                <div class="row">
                                <div class="col-lg-12">
                                        <div class="card-box">
                                            <div class="bar-widget">
                                                <div class="table-box">
                                                    <div class="table-detail"><div class="iconbox bg-primary"><i class="fa fa-cubes"></i></div></div>
                                                    <div class="table-detail text-right"><h4 class="m-t-0 m-b-5"><b class="counter">{{property_count}}</b></h4><p class="text-muted m-b-0 m-t-0">{{label_p_count}}</p></div>
                                                </div>
                                            </div>
                                        </div>
                                </div>
                                <div class="col-lg-12">
                                    <div class="card-box">
                                        <div class="bar-widget">
                                            <div class="table-box">
                                                <div class="table-detail"><div class="iconbox bg-pink"><i class="fa fa-link"></i></div></div>
                                                <div class="table-detail text-right"><h4 class="m-t-0 m-b-5"><b class="counter">{{backlink_count}}</b></h4><p class="text-muted m-b-0 m-t-0">{{label_blc_count}}</p></div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                </div>
                            </div>
                            <div class="col-lg-4">
                                <div class="card-box">
                                    <div id="donut-chart">
                                        <div id="morris-donut-chart-1" style="height: 150px;"></div>
                                    </div>
                                    <div class="text-center">
                                        <p style="margin: -5px;"><i class="fa fa-circle m-r-5" style="color: #006400;"></i>{{label_website_price}} {{total_website_price}}</p>
                                    </div>
                                </div>
                            </div>   
                        </div>
                            
                         <div class="row">
                             <div class="col-lg-12">
                                 <div class="card-box">
                                  <div class="site-ranking-trust m-b-20">
                                        <div class="p-10 bg-inverse m-b-20">
                                            <h4 class="header-title text-white"><b>
                                                    {{label_backlink_tracking_graph_title}}
                                                </b></h4>
                                            <p class="text-muted font-13">
                                                {{label_backlink_tracking_graph_subtitle}}
                                            </p>
                                        </div>
                                        {% if graph is empty %}
                                            <div class="alert alert-warning"><i class="fa fa-warning"></i> {{no_backlink_found}}</div>
                                        {% else %}
                                            <div id="website_ranking_trust_graph" class=" m-b-10"></div>
                                        {% endif %}
                                    </div>
                                    </div>
                             </div>
                         </div>
                        
                        
                        {#main content end#}
                    </div> {#container end#}
                </div> {#content end#}
                {#include footer#}
                {% include '@CsSeoMegaBundlePack/Global/includes/footer.twig' %} 
            </div> {#page content end#}
        </div> {#wrapper end#}
    </div>
</div> {#page end#}