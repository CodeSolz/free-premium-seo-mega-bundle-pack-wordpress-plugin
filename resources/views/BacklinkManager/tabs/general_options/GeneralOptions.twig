{#page start#}
<div class="all-in-one-seo-content">
    <div class="fixed-left">
        {#wrapper start#}
        <div id="wrapper">
            {#include page header#}
            {% include '@CsSeoMegaBundlePack/BacklinkManager/common/header.twig' %} 
            {#include page sidebar#}
            {% include '@CsSeoMegaBundlePack/BacklinkManager/common/sidebar.twig' %} 
            {#content page start#}
            <div class="content-page">
                {#content div start#}
                <div class="content">
                    {#container div start#}
                    <div class="container">
                        {#page title start#}
                        <div class="row">
                            <div class="col-sm-12">
                                <h4 class="page-title">{{page_title}}</h4>
                                <p class="text-muted page-title-alt">{{page_subtitle}}</p>
                            </div>
                        </div>
                        {#page title end#}
                            <div class="row">
                                <div class="col-lg-12">
                                    <div id="general_options" class="card-box">
                                        <div class="row">
                                            <div class="col-lg-6">
                                                {#<h4 class="m-t-0 header-title">Add a website</h4>#}
                                                <p class="text-muted m-b-30 font-13">{{panel_subtitle}}</p>
                                                <form id="FormBacklinkOptions" action="{{form_action}}" role='form' method='post'>
                                                    <div class="form-group">
                                                        <label for="add new property">{{label_self_pinging}}</label><Br>
                                                        <div class="radio radio-success">
                                                            <input type="radio" name="input_self_pingback" id="input_self_pingback" value="on" {% if get_options.self_ping == 'on' or get_options.self_ping is not defined %} checked="checked" {% endif%}>
                                                            <label for="radio1">{{label_on}}</label>
                                                        </div>
                                                        <div class="radio radio-pink">
                                                            <input type="radio" name="input_self_pingback" id="input_self_pingback" value="off" {% if get_options.self_ping == 'off' %} checked="checked" {% endif%}>
                                                            <label for="radio2">{{label_off}}</label>
                                                        </div>
                                                    </div>
                                                    <div class="form-group">
                                                        <label for="add new property">{{label_click_tracking}}</label><Br>
                                                        <div class="radio radio-success">
                                                            <input type="radio" name="input_click_tracking" id="input_click_tracking" value="on" {% if get_options.click_tracking == 'on'  %} checked="checked" {% endif%}>
                                                            <label for="radio1">{{label_on}}</label>
                                                        </div>
                                                        <div class="radio radio-pink">
                                                            <input type="radio" name="input_click_tracking" id="input_click_tracking" value="off" {% if get_options.click_tracking == 'off' or get_options.click_tracking is not defined %} checked="checked" {% endif%}>
                                                            <label for="radio2">{{label_off}}</label>
                                                        </div>
                                                    </div>
                                                    
                                                    
                                                    <br>
                                                        <br>
                                                        <input type="hidden" id="loading_gif" value="{{loading_gif}}"/> 
                                                        <input type="hidden" name="action" id="action" value="CSGeneralOptoins"/>
                                                        <input type="hidden" name="_wpnonce" id="_wpnonce" value="{{nonce_field}}"/>
                                                    <button class="btn btn-purple waves-effect waves-light btn-submit" type="submit">{{label_submit_btn}} <i class="fa fa-angle-double-right"></i></button>
                                                </form>
                                            </div>
                                            <div class="col-lg-5 col-lg-offset-1">
                                                <div class="panel-group" id="accordion-test-2">
                                                    <div class="panel panel-default faq-panel">
                                                        <div class="panel-heading">
                                                            <h4 class="panel-title">
                                                                <a data-toggle="collapse" data-parent="#accordion-test-2" href="#collapseOne-2" aria-expanded="false" class="collapsed">
                                                                    {{faq_title}}
                                                                </a>
                                                            </h4>
                                                        </div>
                                                        <div id="collapseOne-2" class="panel-collapse collapse">
                                                            <div class="panel-body">
                                                                <ul>
                                                                    {% for ques,ans in faq_ques_ans%}
                                                                        <li>
                                                                            {{ques}}
                                                                            <ul>
                                                                                <li>{{ans}}</li>
                                                                            </ul>
                                                                        </li>
                                                                    {% endfor %}
                                                                </ul>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        
                        {#main content start#}
                        
                        {#main content end#}
                    </div> {#container end#}
                </div> {#content end#}
                {#include footer#}
                {% include '@CsSeoMegaBundlePack/BacklinkManager/common/footer.twig' %} 
            </div> {#page content end#}
        </div> {#wrapper end#}
    </div>
</div> {#page end#}