{#page start#}
<div class="all-in-one-seo-content">
    <div class="fixed-left">
        {#wrapper start#}
        <div id="wrapper">
            {#include page header#}
            {% include '@CsSeoMegaBundlePack/BacklinkManager/common/header.twig' %} 
            {#include page sidebar#}
            {% include '@CsSeoMegaBundlePack/BacklinkManager/common/sidebar.twig' %} 
            {#content page start#}
            <div class="content-page">
                {#content div start#}
                <div class="content">
                    {#container div start#}
                    <div class="container">
                        {#page title start#}
                        {% include '@CsSeoMegaBundlePack/BacklinkManager/common/title.twig' %} 
                        {#page title end#}
                            <div class="row">
                                <div class="col-lg-12">
                                    <div class="card-box">
                                        {% if filter_title != '' %}
                                        <div class="p-10 bg-inverse m-b-20">
                                            <h4 class="header-title text-white"><b>
                                                    {{filter_title|raw}}
                                                </b></h4>
                                            <p class="text-muted font-13">
                                                {{filter_subtitle|raw}}
                                            </p>
                                        </div>
                                        {% endif %}
                                        <div class="">
                                            <input type="hidden" id="action" value="{{form_action}}"/> 
                                            <input type="hidden" name="_wpnonce" id="_wpnonce" value="{{nonce_field}}"/>
                                        </div>
                                        <div class="button-list btn-group" id="demo-delete-row">
                                            <div class="btn-group" >
                                                <button type="button" class="btn btn-white dropdown-toggle waves-effect " data-toggle="dropdown" aria-expanded="false">{{label_filter}} <span class="caret"></span></button>
                                                <ul class="dropdown-menu" >
                                                    {% for value,name in filters %}
                                                        <li><a href="{{current_url}}&filter={{value}}" >{{name}}</a></li>
                                                    {% endfor %}
                                                        
                                                </ul>
                                            </div>
                                            <div class="btn-group" >
                                                <button type="button" class="btn btn-warning dropdown-toggle waves-effect" data-toggle="dropdown" aria-expanded="false">{{actn_btns}} <span class="caret"></span></button>
                                                <ul class="dropdown-menu">
                                                    <li><a id="make_active">{{actn_btn_active}}</a></li>
                                                    <li><a id="make_inactive">{{actn_btn_inactive}}</a></li>
                                                </ul>
                                            </div>
                                            <button id="btn_delete" data-type="redirects_rules" data-action="bl_delete_link" class="btn btn-danger" disabled="disabled"><i class="fa fa-times m-r-5"></i>{{actn_delete_btn}}</button>
                                        </div>
                                        <table id="demo-custom-toolbar" data-toggle="table" data-toolbar="#demo-delete-row" data-search="true" 
                                                data-show-columns="true" data-sort-name="id"
                                               data-page-list="[10, 20, 30, 50]" data-page-size="10" data-pagination="true" data-show-pagination-switch="true" class="table-bordered">
                                            <thead>
                                                <tr >
                                                    <th data-field="state" data-checkbox="true"></th>
                                                    {% for col_name in tbl_headers %}
                                                        <th data-field="{{ col_name }}" data-sortable="true" >{{ col_name }}</th>
                                                    {% endfor %}
                                                </tr>
                                            </thead>
                                            <tbody>
                                                {% for data in tbl_data_array %}
                                                    <tr id="item_id_{{data.id|e}}" class="" >
                                                        <td><input  type="checkbox" value="{{data.id|e}}"/>
                                                        </td>
                                                        
                                                        <td class="actions">
                                                            <a class="btn btn-xs btn-default " id="" data-item_id="{{data.ref_id|e}}" data-action="bl_edit_link"><i class="fa fa-edit"></i></a>
                                                            <a class="remove-row btn btn-xs btn-danger "  id="" data-item_id="{{data.ref_id|e}}" data-action="bl_delete_link"><i class="fa fa-trash"></i></a>
                                                            {% if data.ref_id|e == 448 %}
                                                            <a href="#" class="btn-delete-link" >d</a>
                                                            {% endif %}
                                                        </td>
                                                    </tr>
                                                {% endfor %}
{#                                                            <a class="btn btn-xs btn-danger btn-delete-link"  id="" data-toggle="tooltip" data-title="Remove Link" data-item_id="1" data-action="bl_delete_link"><i class="fa fa-trash"></i></a>#}
                                                </tbody>
                                        </table>
                                        {% if filter_title != '' %}
                                            <br><br>
                                            <a href="{{current_url}}" class="btn btn-primary" ><i class="fa fa-angle-double-left"></i> {{label_back_to_btn}}</a>
                                        {% endif %}
                                    </div>
                                </div>
                            </div>
                        
                        {#main content start#}
                        
                        {#main content end#}
                    </div> {#container end#}
                </div> {#content end#}
                {#include footer#}
                {% include '@CsSeoMegaBundlePack/BacklinkManager/common/footer.twig' %} 
            </div> {#page content end#}
        </div> {#wrapper end#}
    </div>
</div> {#page end#}

