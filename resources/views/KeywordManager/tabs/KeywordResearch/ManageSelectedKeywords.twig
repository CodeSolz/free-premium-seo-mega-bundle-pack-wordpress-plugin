{#page start#}
<div class="all-in-one-seo-content">
    <div class="fixed-left">
        {#wrapper start#}
        <div id="wrapper">
            {#include page header#}
            {% include '@CsSeoMegaBundlePack/Global/includes/header.twig' %} 
            {#include page sidebar#}
            {% include '@CsSeoMegaBundlePack/Global/includes/sidebar.twig' %} 
            {#content page start#}
            <div class="content-page">
                {#content div start#}
                <div class="content">
                    {#container div start#}
                    <div class="container">
                        {#page title start#}
                        {% include '@CsSeoMegaBundlePack/Global/includes/title.twig' %} 
                        {#page title end#}
                        {#main content start#}
                        <div class="row">
                            <div class="col-lg-12">
                                <div class="card-box">
                                {% include '@CsSeoMegaBundlePack/Global/includes/no_item_found.twig' %} 
                                
                                <form id="InternalAddNewRule" action="{{form_action}}" role='form' method='post' class="form-horizontal">
                                <div class="btn-group" id="demo-delete-row">
                                    <button id="btn_delete" data-action="selected_keyword_delete" class="btn btn-danger" disabled="disabled"><i class="fa fa-times m-r-5"></i>{{actn_delete_btn}}</button>
                                    <button id="btn_compare" class="btn btn-inverse" disabled="disabled" type="submit"><i class="fa fa-download m-r-5"></i>{{actn_btn_export}}</button>
                                    <input type="hidden" name="_wpnonce" id="_wpnonce" value="{{nonce_field}}"/>
                                </div>
                                   
                                <table id="demo-custom-toolbar" data-toggle="table" data-toolbar="#demo-delete-row" data-search="true" 
                                        data-show-columns="true" data-sort-name="id"
                                       data-page-list="[10, 20, 30, 50]" data-page-size="10" data-pagination="true" data-show-pagination-switch="true" class="table-bordered">
                                    <thead>
                                        <tr >
                                            <th data-field="state" data-checkbox="true"></th>
                                            {% for col_name in tbl_headers %}
                                                <th data-field="{{ col_name }}" data-sortable="true">{{ col_name }}</th>
                                            {% endfor %}
                                        </tr>
                                    </thead>
                                    <tbody>
                                        {% for row in tbl_rows %}
                                            <tr id="item_id_{{row.id}}" >
                                                <td><input  type="checkbox" name="keywords[{{row.group}}][]" value="{{row.keyword}}"/></td>
                                                <td>
                                                    <input type="hidden" value="{{row.keyword}}__{{row.group}}" class="item_id"/>
                                                    <input type="hidden" name="keywords[{{row.group}}][]" value="{{row.keyword}}"/>
                                                    <a href="#">{{row.keyword}}</a>
                                                </td>
                                                <td>
                                                    {{row.group}}
                                                </td>
                                            </tr>
                                        {% endfor %}
                                        </tbody>
                                </table>
                                </form>
                            </div>
                            </div>
                        </div>
                        
                        {#main content end#}
                    </div> {#container end#}
                </div> {#content end#}
                {#include footer#}
                {% include '@CsSeoMegaBundlePack/Global/includes/footer.twig' %} 
            </div> {#page content end#}
        </div> {#wrapper end#}
    </div>
</div> {#page end#}